# Do not edit this file manually!
# It will be overwritten by ALTER SYSTEM command.

# Global:
{% if item.use_global_postgresql_auto_conf|default(true,true) %}
{%   for k,v in postgresql_instance_global_postgresql_auto_conf.items()|sort %}
{{ k }} = {{ v }}
{%   endfor %}
{% endif %}

# Custom:
{% if item.custom_postgresql_auto_conf is defined %}
{%   for k,v in item.custom_postgresql_auto_conf.items()|sort %}
{{ k }} = {{ v }}
{%   endfor %}
{% endif %}

